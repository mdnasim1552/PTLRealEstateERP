                    $(this).attr('data-click-state', 1)
                    openNav();
                }

            });

            var menuicon = document.querySelectorAll(".menu-link");

            menuicon.forEach(function (item, index) {
                item.addEventListener("click", function () {
                    openNav();
                    functionIsRunning = 1;
                })
            })

           // setTimeout($("#AlertArea").fadeOut(5000), 5000);
