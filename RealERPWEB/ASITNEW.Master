        function HideLabel(id) {


            if (id == "0") {
              
                $('#lblmsg').removeClass("Message");
                $('#lblmsg').addClass("msgerror");
            }
            else {
                $('#lblmsg').addClass("msgsuccess");
            }
            setTimeout("lblHide()", 3000);
